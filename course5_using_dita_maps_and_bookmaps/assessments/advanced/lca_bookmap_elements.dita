  </critdates>
 </prolog>
 <learningAssessmentbody>
  <lcInteraction>
   <lcSequencing id="bookmaps_1">
    <lcQuestion>Arrange the following elements into the sequence in which they are allowed within
     the &lt;bookmap> element.</lcQuestion>
    <lcSequenceOptionGroup id="lcSequenceOptionGroup_qyy_lny_kv">
     <lcSequenceOption>
      <lcAnswerContent>&lt;booktitle></lcAnswerContent>
      <lcSequence value="1"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;bookmeta></lcAnswerContent>
      <lcSequence value="2"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;frontmatter></lcAnswerContent>
      <lcSequence value="3"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;chapter></lcAnswerContent>
      <lcSequence value="4"/>
     </lcSequenceOption>
     <lcSequenceOption>
      <lcAnswerContent>&lt;backmatter></lcAnswerContent>
      <lcSequence value="5"/>
     </lcSequenceOption>
    </lcSequenceOptionGroup>
   </lcSequencing>
  </lcInteraction>
 </learningAssessmentbody>
</learningAssessment>
